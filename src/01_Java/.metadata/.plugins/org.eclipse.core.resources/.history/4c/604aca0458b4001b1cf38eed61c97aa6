package com.lect.ex0_janmuri;
class Person{
	private String name;
	private int age;
	private char gender; // 'm' 'f'
	public Person() {} 
	public Person(String name, int age, char gender) {
		this.name = name;
		this.age  = age;
		this.gender = gender;
	}
}
public class PersonTestMain {
	public static void main(String[] args) {
		Person kim = new Person("È«±æµ¿",20,'m');
	}
}
