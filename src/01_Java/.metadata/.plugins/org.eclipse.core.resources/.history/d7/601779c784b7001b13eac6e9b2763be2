package com.lect.ex11_account;
public class CreditLineAccount extends CheckingAccount {
	private int creditLine; // 카드 한도
	public CreditLineAccount(String accountNo, String ownerName, String cardNo,
			int creditLine) {
		super(accountNo, ownerName, cardNo);
		this.creditLine = creditLine;
	}
	public CreditLineAccount(String accountNo, String ownerName, int balance,
			String cardNo, int creditLine) {
		super(accountNo, ownerName, balance, cardNo);
		this.creditLine = creditLine;
	}
	// kim = new CreditLineAccount("11-11","홍길동",1000,"1111-2222-3333-4444",2000);
	// kim.pay(1500);
}









