package com.lect.ex05_lunch;

public class Child2 extends LunchMenu{
	public Child2(String typeString, int rice, int bulgogi, int soup, int banana, int milk, int almond) {
		super(typeString, rice, bulgogi, soup, banana, milk, almond);
	}
	@Override
	public int calculate() {
		return getRice()+getBulgogi()+getSoup()+getMilk();
	}
}
