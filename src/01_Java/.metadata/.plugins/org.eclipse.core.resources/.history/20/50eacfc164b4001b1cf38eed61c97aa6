package com.lect.ex1_student;

public class Student {
	private String name;
	private int kor;
	private int eng;
	private int mat;
	private int tot;
	private double avg;
	public Student(String name, int kor, int eng, int mat) {
		this.name = name; this.kor = kor;
		this.eng = eng;   this.mat = mat;
		tot = kor+eng+mat;
		avg = tot/3.0;
	  }
}
