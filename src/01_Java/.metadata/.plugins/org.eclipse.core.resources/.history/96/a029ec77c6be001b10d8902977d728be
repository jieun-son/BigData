package com.lect.ex2_dataInputStreamDataOutputStream;
public class Product {
	private String name;
	private int    price;
	private int    ps;
	public Product() { }
	public Product(String name, int price, int ps) {
		this.name = name;
		this.price = price;
		this.ps = ps;
	}
	@Override
	public String toString() {
		return String.format("%s\t%d¿ø\t%d°³\n", name, price, ps);
	}
}
